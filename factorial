import java.util.*;
public static voidmain(String[]args){
    int f=1;
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the numbrer to find factorial");
    int n=sc.nextInt();
    for (int i=1;i<=n;i++){
        f=f*i
    }
    System.out.println("Factorial of +n+"is f);
}